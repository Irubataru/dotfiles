alias reload!='. ~/.zshrc'

# Useful ls aliases
alias ls='ls --group-directories-first --color=auto --hide="*~"'
alias la='ls -a'
alias ll='ls -l'
alias lla='ls -al'
alias rm='rm -v'
alias mv='mv -v'
alias cp='cp -v'
alias vim='nvim'
alias octave='/usr/bin/octave-cli'
alias octave-gui='/usr/bin/octave'
